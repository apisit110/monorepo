{
  "name": "monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:portal": "turbo run dev --filter=mylab-portal*",
    "test": "turbo run lint",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:portal": "turbo run dev --filter=mylab-portal*"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "husky": "^8.0.0",
    "prettier": "^2.8.4",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.19.2"
}
